generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id  Int   @id @default(autoincrement())
  first_name String 
  last_name String
  password String
  email String? 
}

model Product {
  id Int @id @default(autoincrement())
  title String 
  description String 
  createdAt DateTime @default(now())
}
